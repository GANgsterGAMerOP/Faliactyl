              <div class="col-md-6 grid-margin stretch-card">
                <div class="card">
                  <div class="card-body">
                    <form class="forms-sample" action="/gift">
                      <div class="form-group">
                        <label style="font-weight: bold;">Gift Resources</label><BR>
                      </div>
                      <button class="badge badge-outline-green" style="padding: 12px 30px; font-size: 15px;" type="submit">Gift Resources</button>
                    </form>
                  </div>
                </div>
              </div>
              <div class="col-md-6 grid-margin stretch-card">
                <div class="card">
                  <div class="card-body">
                    <form class="forms-sample" action="/redeem">
                      <div class="form-group">
                        <label style="font-weight: bold;">Redeem Coupon</label><BR>
                      </div>
                      <button class="badge badge-outline-green" style="padding: 12px 30px; font-size: 15px;" type="submit">Redeem Coupon</button>
                    </form>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <!-- content-wrapper ends -->
          <%- include('./components/footer') %>
        </div>
        <!-- main-panel ends -->
      </div>
      <!-- page-body-wrapper ends -->
    </div>
